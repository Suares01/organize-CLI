{
  "env": {
    "commonjs": true,
    "es2021": true,
    "node": true
  },
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:prettier/recommended"
  ],
  "plugins": [
    "prettier",
    "eslint-plugin-import-helpers"
  ],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "camelcase": "off",
    "no-console": "off",
    "prettier/prettier": "error",
    "class-methods-use-this": "off",
    "no-useless-return": "off"
  },
  "settings": {
    "import/resolver": {
      "node": {}
    }
  }
}
